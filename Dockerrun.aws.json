{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "essential": true,
            "image": "postgres",
            "memoryReservation": 256,
            "mountPoints": [
                {
                    "containerPath": "/var/lib/postgresql/data",
                    "sourceVolume": "Pgdata"
                }
            ],
            "name": "db",
            "portMappings": [
                {
                    "containerPort": 5432,
                    "hostPort": 5432
                }
            ]
        },
        {
            "image": "drfaustusfade/testgirls",
            "links": [
              "db"
            ],
            "memoryReservation": 256,
            "command": [
                "bash",
                "-c",
                "python3 manage.py makemigrations &&\n         python3 manage.py migrate &&\n         gunicorn mysite.wsgi -b 0.0.0.0:8000",
                "#",
                "python3",
                "manage.py",
                "runserver",
                "0.0.0.0:8000"
            ],
            "essential": true,
            "name": "web",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 80
                }
            ]
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "pgdata"
            },
            "name": "Pgdata"
        }
    ]
}
